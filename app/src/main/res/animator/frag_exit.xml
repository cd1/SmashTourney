<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android" >
    <objectAnimator
        android:duration="@integer/nav_animation_duration"
        android:propertyName="x"
        android:valueFrom="0"
